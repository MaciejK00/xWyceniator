
import com.example.demo.entity.Land;
global com.example.demo.prices.TypePrice typePrice;

rule "Cena za rolna"
    when
        Land(type == "Rolna")

    then
        typePrice.setPrice(200.0);
end

rule "Cena za budowlana"
    when
        Land(type == "Budowlana")

    then
        typePrice.setPrice(1200.0);
end


